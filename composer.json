{
    "name": "stechstudio/laravel-jwt",
    "description": "Simple helper package that makes it easy to generate and consume JWT tokens",
    "type": "library",
    "require": {
        "php": "^8.0",
        "lcobucci/jwt": "3.2.*|3.3.*",
        "illuminate/support": "^5.4|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "~9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph",
            "email": "joseph@stechstudio.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "STS\\JWT\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "STS\\JWT\\JWTServiceProvider"
            ],
            "aliases": {
                "JWT": "STS\\JWT\\JWTFacade"
            }
        }
    }
}
